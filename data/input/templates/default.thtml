<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title><% if {[info exists pageTitle]} { lindex "$pageTitle | $websiteTitle" } else { lindex $websiteTitle } %></title>
        <link rel="stylesheet" href="main.css">
    </head>
    <body>
        <% lindex "This is $fileName." %>
        <div id="sidebar">
        <ul><% set a {}
            foreach page $pageIndex {
                set a [concat $a "<li><a href=\"$page\">$page</a></li>"]
            }
            lindex $a %></ul>
        </div><!-- #sidebar -->
        <div id="container">
        <% lindex $content %>
        </div><!-- #container -->
    </body>
</html>